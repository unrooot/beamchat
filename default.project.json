{
	"name": "beamchat",
	"tree": {
		"$className": "DataModel",
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"BeamchatLoader": {
				"$path": "src/loader/"
			}
		},
		"ServerScriptService":{
			"$className": "ServerScriptService",
			"beamchat": {
				"$className": "Folder",
				"Modules": {
					"$path": "src/beamchat/modules"
				},
				"Scripts": {
					"$path": "src/beamchat/scripts"
				}
			}
		}
	}
}
